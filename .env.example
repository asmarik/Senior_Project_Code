# .env.example
# ===========================================
# PDF Compliance Checker Configuration Template
# Copy this to .env and adjust values as needed
# ===========================================

# Server Settings
PORT=8000
HOST=0.0.0.0

# LLM Configuration
# Recommended: false for development, true for production
LLM_CLAUSE_MATCHING=true
LLM_RERANKING=true
FORCE_DISABLE_LLM=false

# Performance Settings
MAX_FILE_SIZE=16777216
UPLOAD_FOLDER=uploads

# Qdrant Vector Database
QDRANT_PATH=./qdrant_db
QDRANT_COLLECTION_NAME=pdpl_articles

# Model Configuration
EMBEDDING_MODEL=intfloat/e5-small-v2
LLM_MODEL=Qwen/Qwen2.5-1.5B-Instruct

# Development Mode
DEV_MODE=false
